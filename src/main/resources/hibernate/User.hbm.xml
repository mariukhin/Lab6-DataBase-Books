<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class screenadaptationname="items.ScreenAdaptation" table="UsersH">
        <meta attribute="class-description">
            This class contains developer's details.
        </meta>
        <id screenadaptationname="id" type="int" column="id"/>
        <property screenadaptationname="screenadaptationname" column="screenadaptationname" type="string"/>
        <property screenadaptationname="year" column="year" type="string"/>
        <property screenadaptationname="mark" column="mark" type="int"/>
    </class>
</hibernate-mapping>